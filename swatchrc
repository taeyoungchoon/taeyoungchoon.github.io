#watchfor  /FAN_FAILED/
#       mail=root,subject=Fan error again

watchfor /apr/
        echo

watchfor /AppDirector/
        echo = yellow

watchfor /ISDN/
        echo = green

watchfor /ILPOWER-3-CONTROLLER_PORT_ERR/
        echo = blue

watchfor /XP12000/
        echo = red
        mail=bar@foo.com,subject=XP12000 log from swatch

watchfor /DEBUG/
        echo = blue
        mail=bar@foo.com,subject=DEBUG log from swatch

#end
